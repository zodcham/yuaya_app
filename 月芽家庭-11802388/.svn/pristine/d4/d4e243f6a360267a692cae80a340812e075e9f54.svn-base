.layui-m-layer {
    position: relative;
    z-index: 59891014
}
.layui-m-layer * {
    -webkit-box-sizing: content-box;
    -moz-box-sizing: content-box;
    box-sizing: content-box
}
.layui-m-layermain, .layui-m-layershade {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%
}
.layui-m-layershade {
    background-color: rgba(0,0,0,.7);
    pointer-events: auto
}
.layui-m-layermain {
    display: table;
    font-family: Helvetica, arial, sans-serif;
    pointer-events: none
}
.layui-m-layermain .layui-m-layersection {
    display: table-cell;
    vertical-align: middle;
    text-align: center
}
.layui-m-layerchild {
    position: relative;
    display: inline-block;
    text-align: left;
    /*background-color: #fff;*/
    font-size: 1em;
    border-radius: 0.4em;
    box-shadow: 0 0 0.5em rgba(0,0,0,.1);
    pointer-events: auto;
    -webkit-overflow-scrolling: touch;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both;
    -webkit-animation-duration: .2s;
    animation-duration: .2s
}@-webkit-keyframes layui-m-anim-scale{0%{opacity:0;-webkit-transform:scale(.5);transform:scale(.5)}100%{opacity:1;-webkit-transform:scale(1);transform:scale(1)}}@keyframes layui-m-anim-scale{0%{opacity:0;-webkit-transform:scale(.5);transform:scale(.5)}100%{opacity:1;-webkit-transform:scale(1);transform:scale(1)}}
.layui-m-anim-scale {
    animation-name: layui-m-anim-scale;
    -webkit-animation-name: layui-m-anim-scale
}@-webkit-keyframes layui-m-anim-up{0%{opacity:0;-webkit-transform:translateY(50em);transform:translateY(50em)}100%{opacity:1;-webkit-transform:translateY(0);transform:translateY(0)}}@keyframes layui-m-anim-up{0%{opacity:0;-webkit-transform:translateY(50em);transform:translateY(50em)}100%{opacity:1;-webkit-transform:translateY(0);transform:translateY(0)}}
.layui-m-anim-up {
    -webkit-animation-name: layui-m-anim-up;
    animation-name: layui-m-anim-up
}
.layui-m-layer0 .layui-m-layerchild {
    width: 90%;
    max-width: 40em
}
.layui-m-layer1 .layui-m-layerchild {
    border: none;
    border-radius: 0
}
.layui-m-layer2 .layui-m-layerchild {
    width: auto;
    max-width: 16.25em;
    min-width: 2.5em;
    border: none;
    background: 0 0;
    box-shadow: none;
    color: #fff
}
.layui-m-layerchild h3 {
    padding: 0 0.625em;
    height: 3.75em;
    line-height: 3.75em;
    font-size: 1em;
    font-weight: 25em;
    border-radius: 0.32em 0.32em 0 0;
    text-align: center
}
.layui-m-layerbtn span, .layui-m-layerchild h3 {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap
}
.layui-m-layercont {
    padding: 2em 1em;
    line-height: 1.5em;
    font-size:1em;
    text-align: center;
}
.layui-m-layer1 .layui-m-layercont {
    padding: 0;
    text-align: left
}
.layui-m-layer2 .layui-m-layercont {
    text-align: center;
    padding: 0;
    line-height: 0
}
.layui-m-layer2 .layui-m-layercont i {
    width: 1.56em;
    height: 1.56em;
    margin-left: 0.5em;
    display: inline-block;
    background-color: #fff;
    border-radius: 100%;
    -webkit-animation: layui-m-anim-loading 1.4s infinite ease-in-out;
    animation: layui-m-anim-loading 1.4s infinite ease-in-out;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both
}
.layui-m-layerbtn, .layui-m-layerbtn span {
    position: relative;
    text-align: center;
    border-radius: 0 0 0.3em 0.3em;
}
.layui-m-layer2 .layui-m-layercont p {
    margin-top: 1.25em
}@-webkit-keyframes layui-m-anim-loading{0%,100%,80%{transform:scale(0);-webkit-transform:scale(0)}40%{transform:scale(1);-webkit-transform:scale(1)}}@keyframes layui-m-anim-loading{0%,100%,80%{transform:scale(0);-webkit-transform:scale(0)}40%{transform:scale(1);-webkit-transform:scale(1)}}
.layui-m-layer2 .layui-m-layercont i:first-child {
    margin-left: 0;
    -webkit-animation-delay: -.32s;
    animation-delay: -.32s
}
.layui-m-layer2 .layui-m-layercont i.layui-m-layerload {
    -webkit-animation-delay: -.16s;
    animation-delay: -.16s
}
.layui-m-layer2 .layui-m-layercont > div {
    line-height: 1.375em;
    padding-top: 0.44em;
    margin-bottom: 1.25;
    font-size: 0.8em
}
.layui-m-layerbtn {
    display: box;
    display: -moz-box;
    display: -webkit-box;
    width: 100%;
    height: 3em;
    line-height: 3em;
    font-size: 1em;
    color:#000000;
    border-top: 0.0625em solid #D0D0D0;
    background-color: #F2F2F2
}
.layui-m-layerbtn span {
    display: block;
    -moz-box-flex: 1;
    box-flex: 1;
    -webkit-box-flex: 1;
    font-size: 1em;
    cursor: pointer
}
.layui-m-layerbtn span[yes] {
    color: #40AFFE
}
.layui-m-layerbtn span[no] {
    border-right: 0.0625em solid #D0D0D0;
    border-radius: 0 0 0 0.3em
}
.layui-m-layerbtn span:active {
    background-color: #F6F6F6
}
.layui-m-layerend {
    position: absolute;
    right: 0.44em;
    top: 0.625em;
    width: 1.875em;
    height: 1.875em;
    border: 0;
    font-weight: 25em;
    background: 0 0;
    cursor: pointer;
    -webkit-appearance: none;
    font-size: 1.875em;
}
.layui-m-layerend::after, .layui-m-layerend::before {
    position: absolute;
    left: 0.3125em;
    top: 0.9375em;;
    content: '';
    width: 1.125em;
    height: 0.625em;
    background-color: #999;
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    border-radius: 1.875em;
}
.layui-m-layerend::after {
    transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg)
}
body .layui-m-layer .layui-m-layer-footer {
    position: fixed;
    width: 95%;
    max-width: 100%;
    margin: 0 auto;
    left: 0;
    right: 0;
    bottom: 0.625em;
    background: 0 0
}
.layui-m-layer-footer .layui-m-layercont {
    padding: 1.5em;
    border-radius: 0.3em 0.3em 0 0;
    background-color: rgba(255,255,255,.9)
}
.layui-m-layer-footer .layui-m-layerbtn {
    display: block;
    height: auto;
    background: 0 0;
    border-top: none
}
.layui-m-layer-footer .layui-m-layerbtn span {
    background-color: rgba(255,255,255,.8)
}
.layui-m-layer-footer .layui-m-layerbtn span[no] {
    color: #FD482C;
    border-top: 0.1em solid #c2c2c2;
    border-radius: 0 0 0.3em 0.3em
}
.layui-m-layer-footer .layui-m-layerbtn span[yes] {
    margin-top: 0.8em;
    border-radius: 0.3em
}
body .layui-m-layer .layui-m-layer-msg {
    width: auto;
    max-width: 90%;
    margin: 0 auto;
    bottom: 9.375em;
    background-color: rgba(0,0,0,.7);
    color: #fff
}
.layui-m-layer-msg .layui-m-layercont {
    padding: 0.625em 1.125em;
}


body .layui-m-layer .layui-m-layer-teststatus {
    width: 17em;
}
.layui-m-layer-teststatus .layui-m-layercont {
    background:url(images/open05.png) no-repeat ;
    background-size:100% 100%;
    height:7em;
    color: #000;
    font-family: Arial, "MS Trebuchet", sans-serif;
    font-size:1.2em;
}
.layui-m-layer-teststatus .layui-m-layerbtn {
    background:url(images/btn.png) no-repeat ;
    background-size:100% 100%;
    height:3em;
    border:0;
    margin-top:1em;
}
.layui-m-layer-teststatus .layui-m-layercont .alert_content{
    margin-top:2em;
}
.layui-m-layer-teststatus .layui-m-layerbtn span[no]{
    border:0;
}

.layui-m-layer-showbadge .layui-m-layercont {
    background:url(images/open05.png) no-repeat ;
    background-size:100% 100%;
    height:10em;
    color: #000;
    font-family: Arial, "MS Trebuchet", sans-serif;
    font-size:1.2em;
}
.layui-m-layer-showbadge .layui-m-layercont .alert_content{
    margin-top:1em;
}


.layui-m-layer-showWin .layui-m-layercont {
    background:url(../../../images/space/bg_win.jpg) no-repeat ;
    background-size:100% 100%;
    min-height:10em;
    color: #000;
    font-family: Arial, "MS Trebuchet", sans-serif;
    font-size:1.2em;
}
.layui-m-layer-showWin .layui-m-layercont .alert_content{
    margin-top:2em;
}




.layui-m-layer-showShelf .layui-m-layercont {
    background:url(../../../images/space/bg_win.jpg) no-repeat ;
    background-size:100% 100%;
    min-height:30em;
    color: #000;
    font-family: Arial, "MS Trebuchet", sans-serif;
    font-size:1em;
    padding:1em;
}
.layui-m-layer-showShelf .layui-m-layercont .alert_content{
    margin-top:0em;
}










