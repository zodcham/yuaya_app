var isIPhoneX = function(fn){
    var u = navigator.userAgent;
    var isIOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/); //ios终端
    if (isIOS) {        
        if (screen.height == 812 && screen.width == 375){
            //是iphoneX
            try{
                $(".uh").css("padding","2em 0 0");
                $("#footer").css("padding",".55em 0 1em");
            }catch(e){}
        }else{
            //不是iphoneX
        } 
    }
}

