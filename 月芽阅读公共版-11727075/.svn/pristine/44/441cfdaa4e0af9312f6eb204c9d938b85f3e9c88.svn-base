    <body class="um-vp sc-bg" style="padding-bottom: 0.1em;background: url(../images/read/body.png) fixed;" ontouchstart>
        <div class="ub ub-fh ub-fv ub-ver" style="width:90% !important;margin:0 auto; font-size:36px;">
            <div id="messageList" class="ub ub-fh ub-fv ub-ver"></div>
        </div>

        <div id="item" class="ub ub-fh item uhide ub-ver bc-border ubb">
            <div class="ub ub-f1 uinn ub-ver">
                <div class="ub ub-fh umar-t ub-ac">
